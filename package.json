{
  "name": "starterkit",
  "version": "1.0.0",
  "description": "javascript development env",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development",
    "prestart": "node ./dist/startMessage.js",
    "lint": "esw webpack.config.* src ./src",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "coverage": "jest --coverage",
    "share": "lt --port 8080",
    "eslint": "./node_modules/.bin/eslint ",
    "open:src": "nodemon ./dist/server.js",
    "start": "npm-run-all --parallel build open:src share lint:watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir/__mocks__/styleMocks.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/john22n/starterKit.git"
  },
  "author": "John Noriega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/john22n/starterKit/issues"
  },
  "homepage": "https://github.com/john22n/starterKit#readme",
  "dependencies": {
    "express": "^4.17.1",
    "install": "^0.13.0",
    "logger": "0.0.1",
    "npm": "^6.11.2",
    "open": "^6.4.0",
    "path": "^0.12.7",
    "require-extension": "0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-watch": "^6.0.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-node-externals": "^1.7.2"
  }
}
