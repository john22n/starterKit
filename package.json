{
  "name": "starterkit",
  "version": "1.0.0",
  "description": "javascript development env",
  "main": "index.js",
  "scripts": {
    "buildDev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "prestart": "node ./dist/startMessage.js",
    "lint": "esw webpack.config.* src ./src",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "coverage": "jest --coverage",
    "share": "lt --port 8084",
    "eslint": "./node_modules/.bin/eslint ",
    "open:src": "node ./dist/server.js",
    "start": "npm-run-all open:src share",
    "generate-mock-data": "node ./dist/genMockData.js",
    "prestart-mock": "npm run generate-mock-data",
    "start-mock": "json-server --watch src/api/db.json --port 3002"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir/__mocks__/styleMocks.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/john22n/starterKit.git"
  },
  "author": "John Noriega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/john22n/starterKit/issues"
  },
  "homepage": "https://github.com/john22n/starterKit#readme",
  "dependencies": {
    "express": "^4.17.3",
    "install": "^0.13.0",
    "logger": "0.0.1",
    "npm": "^6.11.2",
    "open": "^6.4.0",
    "path": "^0.12.7",
    "require-extension": "0.0.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-watch": "^6.0.0",
    "faker": "^4.1.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "json-schema-faker": "^0.5.0-rc17",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-node-externals": "^1.7.2"
  }
}
